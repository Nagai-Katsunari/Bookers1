<span id="title">
  <h1>Books</h1>
</span>
<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Body</th>
    </tr>
  </thead>
  <tbody>
    <% @books.each do |book| %>
    <tr>
      <td><%= book.title %></td>
      <td><%= book.body %></td>
      <td> <%= link_to "show", book_path(book) %> </td>
      <td><%= link_to "edit", edit_book_path(book) %> </td>
      <td><%= link_to "Destroy", book, method: :delete, "data-confirm" => "本当に削除しますか？" %></td>
     
    </tr>
    <% end %>
  </tbody>
</table>